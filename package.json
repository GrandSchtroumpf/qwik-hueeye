{
  "name": "qwik-hueeye",
  "version": "0.0.28",
  "description": "UI library for Qwik",
  "main": "./lib/index.qwik.mjs",
  "qwik": "./lib/index.qwik.mjs",
  "types": "./lib-types/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.qwik.mjs",
      "require": "./lib/index.qwik.cjs",
      "types": "./lib-types/index.d.ts"
    }
  },
  "files": [
    "lib",
    "lib-types"
  ],
  "engines": {
    "node": ">=15.0.0"
  },
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.lib": "vite build -c adapters/lib/vite.config.ts --mode lib",
    "build.server": "vite build -c adapters/static/vite.config.ts",
    "build.types": "tsc --emitDeclarationOnly",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "release": "np",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "peerDependencies": {
    "@builder.io/qwik": ">1.5.0",
    "@builder.io/qwik-city": ">1.5.0"
  },
  "devDependencies": {
    "@builder.io/qwik": "1.10.0",
    "@builder.io/qwik-city": "1.10.0",
    "@builder.io/qwik-worker": "^0.0.1",
    "@oddbird/popover-polyfill": "^0.4.4",
    "@types/eslint": "9.6.0",
    "@types/node": "20.11.30",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "eslint": "8.57.0",
    "eslint-plugin-qwik": "1.10.0",
    "lightningcss": "1.26.0",
    "np": "10.0.7",
    "prettier": "3.3.3",
    "sass": "1.77.8",
    "svgson": "^5.3.1",
    "typescript": "5.5.4",
    "undici": "6.19.0",
    "vite": "5.3.5",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-dts": "3.9.1",
    "vite-tsconfig-paths": "4.3.2"
  },
  "packageManager": "pnpm@9.10.0+sha512.73a29afa36a0d092ece5271de5177ecbf8318d454ecd701343131b8ebc0c1a91c487da46ab77c8e596d6acf1461e3594ced4becedf8921b074fbd8653ed7051c"
}
